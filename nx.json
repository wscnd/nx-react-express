{
  "npmScope": "nx-react-express",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "shared-components": {
      "tags": ["scope:shared"],
      "implicitDependencies": []
    },
    "shared-utils": {
      "tags": ["scope:shared"]
    },
    "store": {
      "tags": ["scope:store"],
      "implicitDependencies": []
    },
    "store-e2e": {
      "tags": ["scope:store"],
      "implicitDependencies": ["store"]
    },
    "store-ui": {
      "tags": ["scope:store"]
    }
  }
}
